drop table if exists eventOrg_t;

create table eventOrg_t(
   hateGroupName 		varchar(50) not null,
   eventID				int(6) not null,
   eventDate			date not null, /*need to make date into eventDate to avoid SQL errors */
   constraint eventOrg_pk primary key(hateGroupName, eventID, eventDate),
   constraint eventOrg_fk1 foreign key(hateGroupName) references hategroup_t(name),
   constraint eventOrg_fk2 foreign key(eventID) references event_t(eventID));


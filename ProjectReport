drop table if exists event_t;

create table event_t(
   eventID 				int(6) not null,
   eType 				varchar(20), /*need to make type into eType to avoid SQL errors */
   city					varchar(15),
   stateName 			varchar(15), /*need to make state into stateName to avoid SQL errors */
   numOfAttendees		int(6),
   ideology				varchar(40) not null,
   constraint event_pk primary key(eventID),
   constraint event_fk1 foreign key(ideology) references ideology_t(ideologyName));


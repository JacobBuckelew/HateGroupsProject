commit;

drop table if exists hategroup_t;

create table hategroup_t(
   name 			varchar(60) not null,
   numOfCrimes 		int(6),
   hq 				varchar(32),
   yearFounded 		int(4),
   ideology 		varchar(50),
   constraint hategroup_pk primary key(name),
   constraint hategroup_fk1 foreign key(ideology) references ideology_t(ideologyName));


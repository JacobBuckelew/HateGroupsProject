drop table if exists hategroupstate_t;

create table hategroupstate_t(
   groupName     	varchar(50) not null, /*need to make state into stateName to avoid SQL errors */
   stateName 	varchar(21) not null,
   constraint hategroupstate_pk primary key(groupName, stateName),
   constraint hategroupstate_fk1 foreign key(groupName) references hategroup_t(name));


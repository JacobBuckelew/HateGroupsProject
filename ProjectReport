insert into ideology_t values('Anti-Muslim', 'islamophobia');
insert into ideology_t values('Black Separatist', 'anti-white');
insert into ideology_t values('Christian Identity', 'anti-semitism');
insert into ideology_t values('General Hate', 'miscellaneous');
insert into ideology_t values('Hate Music', 'white supremacy');

   hateGroupName 	varchar(50) not null,
   dob 				date not null,
   residentState	varchar(21),
   name 			varchar(35) not null,
   constraint leader_pk primary key(hateGroupName, dob, name),
   constraint leader_fk1 foreign key(hateGroupName) references hategroup_t(name));


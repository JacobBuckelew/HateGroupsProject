drop table if exists hategroupcollab_t;

create table hategroupcollab_t(
   hateGroupName     	varchar(50) not null,
   collabGroupName 		varchar(50) not null,
   constraint hategroupcollab_pk primary key(hateGroupName, collabGroupName),
   constraint hategroupcollab_fk1 foreign key(hateGroupName) references hategroup_t(name),
   constraint hategroupcollab_fk2 foreign key(collabGroupName) references hategroup_t(name));


Query/Update File


/* Hate Group Database */
/* Julie Sparks, Maria Morales, Jacob Buckelew, Miriam Scheinblum */
/* HateGroupQuery.sql is a script listing possible queries that can be asked of the hate group database.*/

/* How many hate groups are there in each State (with the states ordered in alphabetical order)? */

select stateName as 'State', count(groupName) as 'Number of hate groups'
from hategroupstate_t
group by stateName
order by stateName;

/* Which state has the most hate groups? */

select stateName as 'State with most hate groups'
from hategroupstate_t
group by stateName
order by count(groupName) desc limit 1;

/* What is the name of the hate group that was involved in the most events? */

select hateGroupName as 'Hate group most involved in events'
from eventOrg_t
group by hateGroupName
order by count(distinct eventID) desc limit 1;

/* Which type of event happened the most? */

select eType as 'Most recurrent event type'
from event_t
group by eType
order by count(eventID) desc limit 1;

/* How many hate groups follow each ideology, in alphabetical order? */

select ideology as 'Ideology', count(name) as 'Number of Groups'
from hategroup_t
group by ideology
order by ideology;

/* Which hate groups collaborated with Nation of Islam? */

select collabGroupName as 'Nation of Islam Collaborators'
from hategroupcollab_t
where hateGroupName = 'Nation of Islam';

/* What are the names of the hate groups with leaders from Florida, in alphabetical order? */

select hateGroupName as 'Groups with Leaders from Florida'
from leader_t
where residentState = 'Florida'
order by hateGroupName;

/* Which hate groups have at least two leaders, in alphabetical order? */

select hateGroupName as 'Groups with 2 or more Leaders'
from leader_t
group by hateGroupName
having count(hateGroupName)>=2
order by hateGroupName;

/* What are the names and years founded of all the hate groups founded after 2015, in alphabetical order? */

select name as 'Hate Group founded after 2015', yearFounded as 'Year founded'
from hategroup_t
where yearFounded > 2015
order by name;


/* Which ideologies have a bias type of anti-semitism, in alphabetical order? */

select ideologyName as 'Ideology with anti-semitism bias'
from ideology_t
where biasType = 'anti-semitism'
order by ideologyName;

/* How many events, if any, were held for each ideology? */

select ideology as 'Ideology', count(eventID) as 'Number  of events  held'
from event_t
group by ideology;

/* Which hate groups have names that include America or American, in alphabetical order? */

select name as 'Groups with America in Name'
from hategroup_t
where name like '%America%'
order by name;

/* Which hate groups operate in the Pacific Northwest (CA, OR, WA)? */

select distinct groupName as 'Groups Operating in Pacific Northwest (CA, OR, WA)'
from hategroupstate_t
where stateName in ('California', 'Oregon', 'Washington');

/* In what city and state was the event with ID 123409? */

select city, stateName
from event_t
where eventID = 123409;

/* From which states are the hate groups that participated in the event with ID 123409, in alphabetical order? */

select distinct stateName
from hategroupstate_t, hategroup_t, eventorg_t
where eventID = 123409 AND hateGroupName = name AND name = groupName 
order by stateName;

/* Find all information for events held by the Illinois Family Institute. */

select event_t.*, hateGroupName, eventDate
from eventOrg_t, event_t
where hateGroupName = 'Illinois Family Institute' AND event_t.eventID = eventOrg_t.eventID;

/* Find the average number of attendees for all conferences. */

select AVG(numOfAttendees) as 'Average Number of Conference Attendees'
from event_t
where eType = 'Conference';

/* What is the bias type of the group led by Laurie Cardoza-Moore? */

select biasType
from ideology_t, hategroup_t, leader_t
where leader_t.name = 'Laurie Cardoza-Moore' AND hateGroupName = hategroup_t.name AND ideology = ideologyName;

/* How many events were held in October of 2019? */

select count(eventID) as 'Events in October, 2019'
from eventOrg_t
where eventDate like '2019-10-%';

/* What are the names of the leaders whose groups held events with over 100 attendees? */

select distinct name as 'Leader Names'
from leader_t as L, eventOrg_t as Org, event_t as E
where L.hateGroupName = Org.hateGroupName AND Org.eventID = E.eventID AND numOfAttendees > 100;






/* Paul Elam, leader of A Voice for Men, has decided to take a break and
pass on the leadership role to Mike Cernovich, update the database accordingly */

update leader_t
  set name = 'Mike Cernovich'
  where name = 'Paul Elam';


/* All hategroups with neo-nazi ideologies were involved on a crime recently, adding 1 more to their list, update their data accordingly */

update hategroup_t
  set numOfCrimes = numOfCrimes + 1
  where ideology = 'Neo-Nazi';



